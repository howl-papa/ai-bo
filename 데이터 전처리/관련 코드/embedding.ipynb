    "from openai import AzureOpenAI\n",
    "import pandas as pd\n",
    "from tqdm import tqdm\n",
    "import tiktoken\n",
    "import time\n",
    "import numpy as np\n",
    "\n",
    "# 1. Azure OpenAI ì„¤ì •\n",
    "client = AzureOpenAI(\n",
    "    api_key=\"YOUR_AZURE_OPENAI_KEY\",\n",
    "    api_version=\"2024-02-15-preview\",\n",
    "    azure_endpoint=\"https://team05-openai.openai.azure.com\"\n",
    ")\n",
    "\n",
    "DEPLOYMENT_NAME = \"text-embedding-3-large\"\n",
    "MAX_TOKENS = 8000\n",
    "\n",
    "# 2. tiktoken í† í¬ë‚˜ì´ì € ì¤€ë¹„\n",
    "tokenizer = tiktoken.get_encoding(\"cl100k_base\")\n",
    "\n",
    "def count_tokens(text):\n",
    "    return len(tokenizer.encode(text))\n",
    "\n",
    "def split_text_by_tokens(text, max_tokens=MAX_TOKENS):\n",
    "    tokens = tokenizer.encode(text)\n",
    "    chunks = []\n",
    "    for i in range(0, len(tokens), max_tokens):\n",
    "        chunk = tokenizer.decode(tokens[i:i + max_tokens])\n",
    "        chunks.append(chunk)\n",
    "    return chunks\n",
    "\n",
    "# 3. ì„ë² ë”© ìƒì„± í•¨ìˆ˜ (ì‹ ê·œ ë°©ì‹)\n",
    "def get_embedding(text):\n",
    "    try:\n",
    "        response = client.embeddings.create(\n",
    "            input=[text],\n",
    "            model=DEPLOYMENT_NAME\n",
    "        )\n",
    "        return response.data[0].embedding\n",
    "    except Exception as e:\n",
    "        print(\"â—Error:\", e)\n",
    "        return None\n",
    "\n",
    "# 4. NaN/Inf â†’ None ì²˜ë¦¬\n",
    "def safe_embedding(emb):\n",
    "    if emb is None:\n",
    "        return None\n",
    "    return [float(x) if np.isfinite(x) else None for x in emb]\n",
    "\n",
    "# 5. CSV ë¶ˆëŸ¬ì˜¤ê¸° ë° ì „ì²´ ì²­í¬ ìƒì„±\n",
    "df = pd.read_csv(\"smart_chunks.csv\")\n",
    "raw_chunks = df[\'content\'].dropna().tolist()  # NaN ì œê±°\n",
    "final_chunks = []\n",
    "\n",
    "for text in raw_chunks:\n",
    "    if count_tokens(text) <= MAX_TOKENS:\n",
    "        final_chunks.append(text)\n",
    "    else:\n",
    "        final_chunks.extend(split_text_by_tokens(text))\n",
    "\n",
    "print(f\"ğŸ”¹ ìµœì¢… ì²­í¬ ê°œìˆ˜: {len(final_chunks)}\")\n",
    "\n",
    "# 6. ì„ë² ë”© ìˆ˜í–‰\n",
    "embeddings = []\n",
    "for chunk in tqdm(final_chunks):\n",
    "    emb = get_embedding(chunk)\n",
    "    embeddings.append(safe_embedding(emb))\n",
    "    time.sleep(1.2)\n",
    "\n",
    "# 7. ê²°ê³¼ ì €ì¥\n",
    "result_df = pd.DataFrame({\n",
    "    \"chunk\": final_chunks,\n",
    "    \"embedding\": embeddings\n",
    "})\n",
    "\n",
    "result_df.to_csv(\"embedded_chunks.csv\", index=False)\n",
    "print(\"âœ… ì„ë² ë”© ì™„ë£Œ ë° ì €ì¥ë¨: embedded_chunks.csv\")\n"
   ]
