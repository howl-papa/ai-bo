    "        chunk = text\n",
    "    else:\n",
    "        # ê¸´ ë¬¸ì¥ì€ ì²« max_token ë§Œí¼ë§Œ ìë¦„ (ë˜ëŠ” split í›„ ì²« chunkë§Œ ì‚¬ìš©í•´ë„ ë¨)\n",
    "        chunk = split_text_by_tokens(text, MAX_TOKENS)[0]\n",
    "    \n",
    "    emb = get_embedding(chunk)\n",
    "    embeddings.append(safe_embedding(emb))\n",
    "    final_chunks.append(chunk)\n",
    "    time.sleep(1.2)  # ì†ë„ ì œí•œ ëŒ€ì‘\n",
    "\n",
    "# 7. ê²°ê³¼ ê²°í•© ë° ì €ì¥\n",
    "df[\'chunk\'] = final_chunks\n",
    "df[\'embedding\'] = embeddings\n",
    "\n",
    "df.to_csv(\"embedded_articles.csv\", index=False)\n",
    "print(\"âœ… ì™„ë£Œ: embedded_articles.csv ì €ì¥ë¨\")\n"
